{
    "name": "eliurkis/crud",
    "description": "CRUD Generator",
    "homepage": "https://github.com/eliurkis/crud",
    "authors": [
        {
            "name": "Eliurkis Diaz",
            "email": "eliurkis@gmail.com"
        }
    ],
    "keywords": [
        "crud",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">8.2",
        "spatie/laravel-html": "^3.11.0",
        "laravelcollective/html": "^6.4",
        "illuminate/database": "*",
        "spatie/laravel-medialibrary": "^11.7.6"
    },
    "autoload": {
        "psr-4": {
            "Eliurkis\\Crud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eliurkis\\Crud\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Eliurkis\\Crud\\CrudServiceProvider"
            ]
        }
    }
}
